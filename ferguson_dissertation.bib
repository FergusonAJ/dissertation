@article{chanPhageSelectionRestores2016,
  title = {Phage Selection Restores Antibiotic Sensitivity in {{MDR Pseudomonas}} Aeruginosa},
  author = {Chan, Benjamin K. and Sistrom, Mark and Wertz, John E. and Kortright, Kaitlyn E. and Narayan, Deepak and Turner, Paul E.},
