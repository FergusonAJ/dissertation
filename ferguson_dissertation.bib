@misc{morenoTrackableAgentbasedEvolution2024,
  title = {Trackable {{Agent-based Evolution Models}} at {{Wafer Scale}}},
  author = {Moreno, Matthew Andres and Yang, Connor and Dolson, Emily and Zaman, Luis},
