@article{heydukGeneticsConvergentEvolution2019,
  title = {The Genetics of Convergent Evolution: Insights from Plant Photosynthesis},
  shorttitle = {The Genetics of Convergent Evolution},
  author = {Heyduk, Karolina and {Moreno-Villena}, Jose J. and Gilman, Ian S. and Christin, Pascal-Antoine and Edwards, Erika J.},
