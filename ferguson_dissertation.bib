@article{okeIndependentLineagesCommon2019,
  title = {Independent Lineages in a Common Environment: The Roles of Determinism and Contingency in Shaping the Migration Timing of Even- versus Odd-Year Pink Salmon over Broad Spatial and Temporal Scales},
  shorttitle = {Independent Lineages in a Common Environment},
  author = {Oke, Krista B. and Cunningham, Curry J. and Quinn, Thomas P. and Hendry, Andrew P.},
