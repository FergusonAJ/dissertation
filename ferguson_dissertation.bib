@article{silvaOpenIssuesEvolutionary2016,
  title = {Open {{Issues}} in {{Evolutionary Robotics}}},
  author = {Silva, Fernando and Duarte, Miguel and Correia, Lu{\'i}s and Oliveira, Sancho Moura and Christensen, Anders Lyhne},
  year = {2016},
  month = jun,
  journal = {Evolutionary Computation},
  volume = {24},
  number = {2},
  pages = {205--236},
  issn = {1063-6560},
  doi = {10.1162/EVCO_a_00172},
  abstract = {One of the long-term goals in evolutionary robotics is to be able to automatically synthesize controllers for real autonomous robots based only on a task specification. While a number of studies have shown the applicability of evolutionary robotics techniques for the synthesis of behavioral control, researchers have consistently been faced with a number of issues preventing the widespread adoption of evolutionary robotics for engineering purposes. In this article, we review and discuss the open issues in evolutionary robotics. First, we analyze the benefits and challenges of simulation-based evolution and subsequent deployment of controllers versus evolution on real robotic hardware. Second, we discuss specific evolutionary computation issues that have plagued evolutionary robotics: (1) the bootstrap problem, (2) deception, and (3) the role of genomic encoding and genotype-phenotype mapping in the evolution of controllers for complex tasks. Finally, we address the absence of standard research practices in the field. We also discuss promising avenues of research. Our underlying motivation is the reduction of the current gap between evolutionary robotics and mainstream robotics, and the establishment of evolutionary robotics as a canonical approach for the engineering of autonomous robots.},
  keywords = {autonomous robots,bootstrap problem,controller synthesis,deception,Evolutionary robotics,genomic encoding,online evolution,reality gap,research practices},
  file = {/media/austin/samsung_nvme_pop/research/zotero/storage/FUDY4F7G/Silva et al. - 2016 - Open Issues in Evolutionary Robotics.pdf;/media/austin/samsung_nvme_pop/research/zotero/storage/LIXJR2V9/7491531.html}
}

@article{simoesPredictablePhenotypicNot2017,
