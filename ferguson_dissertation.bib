@article{lassigPredictingEvolution2017,
  title = {Predicting Evolution},
  author = {L{\"a}ssig, Michael and Mustonen, Ville and Walczak, Aleksandra M.},
  year = {2017},
  month = feb,
  journal = {Nature Ecology \& Evolution},
