@article{blountGenomicAnalysisKey2012,
  title = {Genomic Analysis of a Key Innovation in an Experimental {{Escherichia}} Coli Population},
  author = {Blount, Zachary D. and Barrick, Jeffrey E. and Davidson, Carla J. and Lenski, Richard E.},
  year = {2012},
