@article{harmsEvolutionaryBiochemistryRevealing2013,
  title = {Evolutionary Biochemistry: Revealing the Historical and Physical Causes of Protein Properties},
  shorttitle = {Evolutionary Biochemistry},
  author = {Harms, Michael J. and Thornton, Joseph W.},
  year = {2013},
  month = aug,
